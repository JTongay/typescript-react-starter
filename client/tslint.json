{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "prettier": true,
    "member-access": false,
    "jsx-boolean-value": false
  }
}
